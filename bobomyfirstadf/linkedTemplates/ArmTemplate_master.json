{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "bobomyfirstadf"
		},
		"LS_1_accountKey": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « accountKey » de « LS_1 »"
		},
		"LS_ASQL_password": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « password » de « LS_ASQL »"
		},
		"LS_1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://bobodev.dfs.core.windows.net/"
		},
		"LS_ASQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "firstmysqlserver.database.windows.net"
		},
		"LS_ASQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "firstsqldb"
		},
		"LS_ASQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "boboadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "bobomyfirstadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_1_accountKey": {
						"value": "[parameters('LS_1_accountKey')]"
					},
					"LS_ASQL_password": {
						"value": "[parameters('LS_ASQL_password')]"
					},
					"LS_1_properties_typeProperties_url": {
						"value": "[parameters('LS_1_properties_typeProperties_url')]"
					},
					"LS_ASQL_properties_typeProperties_server": {
						"value": "[parameters('LS_ASQL_properties_typeProperties_server')]"
					},
					"LS_ASQL_properties_typeProperties_database": {
						"value": "[parameters('LS_ASQL_properties_typeProperties_database')]"
					},
					"LS_ASQL_properties_typeProperties_userName": {
						"value": "[parameters('LS_ASQL_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "bobomyfirstadf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'bobomyfirstadf_ArmTemplate_0')]"
			]
		}
	]
}